{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-throw-literal": "off"
  }
}
